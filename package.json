  "scripts": {
    "update-env": "node update-env.js",
    "migrate-main": "cd apps/node/main-apis && npx sequelize-cli db:migrate",
    "migrate-leave": "cd apps/node/leave-apis && npx sequelize-cli db:migrate",
    "migrate-project-doc": "cd apps/node/project-documents-api && npx sequelize-cli db:migrate",
    "migrate-tasks": "cd apps/node/tasks-apis && npx sequelize-cli db:migrate",
    "migrate-all": "npm run migrate-main && npm run migrate-leave && npm run migrate-project-doc && npm run migrate-tasks",
    "build-main": "npx nx run node-main-apis:build --skip-nx-cache=true",
    "build-tasks": "npx nx run node-tasks-apis:build --skip-nx-cache=true",
    "build-leave": "npx nx run node-leave-apis:build --skip-nx-cache=true",
    "build-project-doc": "npx nx run node-project-documents-api:build --skip-nx-cache=true",
    "build-all-node": "npm run build-main && npm run build-leave && npm run build-tasks && npm run build-project-doc",
    "build-all-develop": "node generate-project-build.js development",
    "build-all-staging": "node generate-project-build.js staging",
    "build-all-production": "node generate-project-build.js production",
    "cy:run": "cd apps/ && cypress run -- --spec cypress/e2e",
    "test:cypress": "bash apps/cypress_script.sh"
  },
